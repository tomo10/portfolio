<LandingPage.projects
  title={gettext("Projects")}
  description={
    gettext("Here are a few projects I have built using Elixir and LiveView (inc. this website)")
  }
  features={[
    %{
      title: "Ad Astra",
      description:
        "Type in any 2 stars and find the distance between them and how long it would take to travel from one to the other. To the stars!",
      icon: :rocket_launch,
      link_to: "/ad-astra"
    },
    %{
      title: "Pangaea",
      description:
        "A conservation crowdfunding platform and planet centric social network. Conservation has no borders",
      icon: :globe_alt,
      link_to: "https://github.com/tomo10/pangaea_pro"
    },
    %{
      title: "AIDA",
      description:
        "An artificially intelligent digital assistant. Utilising Bumblebee and Elixir to build an assistant to help with daily tasks",
      icon: :puzzle_piece,
      link_to: "/"
    }
  ]}
  max_width={max_width}
/>

